{
  "modal": {
    "space-between": {
      "md": {
        "value": "{semantic.space-between.xl}",
        "type": "spacing"
      },
      "sm": {
        "value": "{semantic.padding.sm}",
        "type": "spacing"
      },
      "lg": {
        "value": "{semantic.space-between.xxxl}",
        "type": "spacing"
      }
    },
    "overlay": {
      "value": "{theme.semantic.color.fg.primary}",
      "type": "color",
      "$extensions": {
        "studio.tokens": {
          "modify": {
            "type": "alpha",
            "value": "0.6",
            "space": "hsl"
          }
        }
      }
    },
    "background": {
      "value": "{theme.semantic.color.canvas.surface}",
      "type": "color"
    },
    "padding": {
      "xl": {
        "value": "{semantic.padding.lg}",
        "type": "spacing"
      },
      "lg": {
        "value": "{semantic.padding.md}",
        "type": "spacing"
      }
    },
    "shadow": {
      "value": "{semantic.shadow.level-3}",
      "type": "boxShadow"
    }
  },
  "modal-radius": {
    "value": "{semantic.radius.xl}",
    "type": "borderRadius"
  },
  "icon": {
    "special": {
      "value": "{theme.semantic.color.brand.special-45degree}",
      "type": "color"
    }
  }
